{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["svelte3", "html", "import"],
    "overrides": [
      {
        "files": ["**/*.svelte"],
        "processor": "svelte3/svelte3",
        "rules": {
          "import/first": "off"
        }
      }
    ],    
    "rules": {
    },
    "ignorePatterns": ["public/build/**/*"]
}
